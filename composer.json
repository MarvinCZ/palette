{
  "name": "pavlista/palette",
  "description": "An on-demand image handling and manipulation library",
  "keywords": ["image", "thumbnail", "watermark", "gd", "imagick", "nette"],
  "homepage": "https://github.com/MichaelPavlista/palette",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Michael Pavlista",
      "homepage": "http://www.pavlista.cz/"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.0",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
  },
  "require-dev": {
    "nette/tester": "~1.3",
    "ext-imagick": "*"
  },
  "suggest": {
    "ext-imagick": "For support cmyk images, better performance and Utils/Compare tool."
  },
  "autoload": {
    "classmap": ["src/"]
  }
}